{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
  "src/main/**/*",
  "src/preload/**/*",
    "src/schema/**/*",
    "src/service/**/*"
    ],
  "compilerOptions": {
    "composite": true,
    "moduleResolution": "bundler",
    "types": ["electron-vite/node"],
    "baseUrl": ".",
    "paths": {
      "@service/*": [
        "src/service/*"
      ],
      "@schema/*": [
        "src/schema/*"
      ]
    }
  }
}
